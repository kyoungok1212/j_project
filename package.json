{
  "name": "guitar-practice-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "npm run manifest:real-drum",
    "dev": "vite",
    "dev:all": "concurrently -k -n FE,API -c cyan,green \"npm run dev\" \"npm run dev:worker\"",
    "dev:bootstrap": "npm run db:migrate:local && npm run db:seed:local",
    "prebuild": "npm run manifest:real-drum",
    "build": "vite build",
    "preview": "vite preview",
    "manifest:real-drum": "node scripts/sync-real-drum-manifest.mjs",
    "dev:worker": "wrangler dev worker/index.ts --local",
    "deploy:worker": "wrangler deploy worker/index.ts",
    "db:migrate:local": "wrangler d1 migrations apply guitar_practice_db --local",
    "db:migrate:remote": "wrangler d1 migrations apply guitar_practice_db --remote",
    "db:seed:local": "wrangler d1 execute guitar_practice_db --local --file ./db/seed/dev_seed.sql",
    "db:seed:remote": "wrangler d1 execute guitar_practice_db --remote --file ./db/seed/prod_seed.sql",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vexflow": "^5.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260201.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.1",
    "typescript": "^5.8.2",
    "vite": "^5.4.11",
    "wrangler": "^4.24.3"
  }
}
